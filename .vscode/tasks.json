{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "g++build",
            "command": "/usr/bin/g++",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "${workspaceFolder}/src/debug/output.cpp",
                "${workspaceFolder}/src/main.cpp",
                "${file}",
                "-I${workspaceFolder}/include",
                "-I${fileDirname}",
                "-L${workspaceFolder}/lib",
                "-o",
                "${workspaceFolder}/build/leetcode"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "dependsOn":[],
            "detail": "调试器生成的任务。"
        }
    ],
    "version": "2.0.0"
}